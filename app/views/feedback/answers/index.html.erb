<h1>Listing feedback_answers</h1>

<table>
  <thead>
    <tr>
      <th>Survey answer</th>
      <th>Question</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedback_answers.each do |feedback_answer| %>
      <tr>
        <td><%= feedback_answer.survey_answer_id %></td>
        <td><%= feedback_answer.question_id %></td>
        <td><%= feedback_answer.text %></td>
        <td><%= link_to 'Show', feedback_answer %></td>
        <td><%= link_to 'Edit', edit_feedback_answer_path(feedback_answer) %></td>
        <td><%= link_to 'Destroy', feedback_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer', new_feedback_answer_path %>
