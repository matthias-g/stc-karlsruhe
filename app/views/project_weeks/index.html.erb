<section>
  <h1>Listing project_weeks</h1>
</section>

<section>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Default</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @project_weeks.each do |project_week| %>
        <tr>
          <td><%= project_week.title %></td>
          <td><%= project_week.default %></td>
          <td><%= link_to 'Show', project_week %></td>
          <td><%= link_to 'Edit', edit_project_week_path(project_week) %></td>
          <td><%= link_to 'Destroy', project_week, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Project week', new_project_week_path %>

</section>