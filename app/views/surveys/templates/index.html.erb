<% content_for :pageTitle do %>
    <%= t('layout.title.templateList') %>
<% end %>

<section class="surveys template list-view">
  <h1><%= t('surveys.label.templateList') %></h1>
  <ul>
    <% @templates.each do |template| %>
      <li>
        <%= link_to template.title, template %>
      </li>
    <% end %>
  </ul>

  <br>

  <%= link_to t('surveys.action.newTemplate'), new_surveys_template_path, class: 'btn btn-default' %>
</section>


