<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(location) -%>">
  <span class='title'>
    <%= location.title %>

  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_projects_admin_location_path(location),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.projects_admin_location_path(location),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => location.title),
        :method => :delete %>
  </span>
</li>
