<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(volunteer_type) -%>">
  <span class='title'>
    <%= volunteer_type.title %>

  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_projects_admin_volunteer_type_path(volunteer_type),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.projects_admin_volunteer_type_path(volunteer_type),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => volunteer_type.title),
        :method => :delete %>
  </span>
</li>
